{
  "functions": {
    "api": {
      "runtime": "python3.11",
      "includeFiles": "app/**,apiTT/**",
      "excludeFiles": "app/**/*.pyc,apiTT/**/*.pyc",
      "entrypoint": "app/asgi.py"
    }
  },
  "routes": [
    { "src": "/api/.*", "dest": "/vercel/api" },
    { "src": "/.*", "dest": "/vercel/api" }
  ]
}
